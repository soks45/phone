{
    "name": "phone",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "client:start": "ng serve",
        "___dev_serve___": "",
        "build:watch": "ng build --watch --configuration development",
        "server:serve": "nodemon dist/phone/server/server.mjs",
        "___prod_serve___": "",
        "build": "ng build",
        "server:run": "node dist/phone/server/server.mjs",
        "___lint___": "",
        "lint": "eslint \"**/*.{js,ts,html}\" --quiet --fix",
        "format": "prettier \"**/*.{js,ts,html,css,scss,json}\" --write",
        "lint-format": "npm run format && npm run lint",
        "___database___": "",
        "migrate:dev": "db-migrate up -e dev"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.scss": [
            "prettier --write"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.0.0",
        "@angular/cdk": "^18.0.0",
        "@angular/common": "^18.0.0",
        "@angular/compiler": "^18.0.0",
        "@angular/core": "^18.0.0",
        "@angular/forms": "^18.0.0",
        "@angular/platform-browser": "^18.0.0",
        "@angular/platform-browser-dynamic": "^18.0.0",
        "@angular/platform-server": "^18.0.0",
        "@angular/router": "^18.0.0",
        "@angular/ssr": "^18.0.6",
        "@ng-web-apis/common": "3.0.6",
        "@ng-web-apis/universal": "^3.0.6",
        "@taiga-ui/cdk": "^3.84.0",
        "@taiga-ui/core": "^3.84.0",
        "@taiga-ui/icons": "^3.84.0",
        "@taiga-ui/kit": "^3.84.0",
        "@taiga-ui/styles": "^3.84.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "4.19.2",
        "express-session": "^1.18.0",
        "http-errors": "~1.6.3",
        "joi": "^17.13.3",
        "morgan": "~1.9.1",
        "pg": "^8.12.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.0.6",
        "@angular-eslint/eslint-plugin": "^18.0.0",
        "@angular-eslint/eslint-plugin-template": "^18.0.0",
        "@angular-eslint/schematics": "^18.0.0",
        "@angular-eslint/template-parser": "^18.0.0",
        "@angular/cli": "^18.0.6",
        "@angular/compiler-cli": "^18.0.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.18.0",
        "@types/http-errors": "^2.0.4",
        "@types/joi": "^17.2.3",
        "@types/morgan": "^1.9.9",
        "@types/node": "^18.18.0",
        "@types/pg": "^8.11.6",
        "@typescript-eslint/eslint-plugin": "6.7.4",
        "@typescript-eslint/parser": "6.7.4",
        "db-migrate": "^0.11.14",
        "db-migrate-pg": "^1.5.2",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-unused-imports": "^3.0.0",
        "husky": "4.3.0",
        "lint-staged": "11.2.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.2"
    }
}
